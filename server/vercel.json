{
    "version": 2,
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "src/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/index.ts"
        },
        {
            "src": "/health",
            "dest": "src/index.ts"
        }
    ],
    "functions": {
        "src/index.ts": {
            "maxDuration": 30
        }
    }
}